<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
  </head>
  <style>
    table, th, td {
      border:1px solid black;
    }
    </style>
  <body>
    Welcome <%= name %><br>

      <table >
        <tr>
          <th>id</th>
          <th>name</th>
          <th>address</th>
          <th>contact</th>
        </tr>
      <tr>
        <td> <%= shop.id %></td>
        <td> <%= shop.name %></td>
        <td> <%= shop.address %></td>
        <td> <%= shop.contact %></td>
       </tr>
      </table>
      <table >
        <tr>
          <th>Name</th>
          <th>Price</th>
          <th>Quantity</th>
        </tr>
      <% for(var i=0; i<product.length; i++) {%>
        <tr>
        <td> <%= product[i].name %></td>
        <td> <%= product[i].price %></td>
        <td> <%= product[i].quantity %></td>
       </tr>
     <% } %>      
    </table>
  </body>
</html>
